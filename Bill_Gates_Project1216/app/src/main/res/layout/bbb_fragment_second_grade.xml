<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="0dp"
    app:cardBackgroundColor="@color/white"
    app:cardCornerRadius="8dp"
    app:cardElevation="10dp"
    app:cardMaxElevation="10dp"
    app:cardPreventCornerOverlap="true"
    app:cardUseCompatPadding="true"
    android:id="@+id/layoutCardview">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <CheckedTextView
                android:id="@+id/checktext1"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:text="선형대수학"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:id="@+id/checktext2"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:text="초급프로젝트"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:id="@+id/checktext3"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:text="자료구조"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext4"
                android:text="객체지향프로그래밍"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext5"
                android:text="논리설계"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext6"
                android:text="C++리눅스프로그래밍"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext7"
                android:text="확률과통계"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext8"
                android:text="알고리즘"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext9"
                android:text="모바일프로그래밍"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext10"
                android:text="프로그램구조와해석"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext11"
                android:text="컴퓨터구조"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>
            <CheckedTextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/checktext12"
                android:text="데이터통신"
                android:layout_marginStart="10dp"
                android:layout_margin="10dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                android:gravity="center"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"/>

        </LinearLayout>

    </ScrollView>


</androidx.cardview.widget.CardView>